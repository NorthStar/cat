<%= form_for([@department,@booklist]) do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :intro %><br>
    <%= f.text_area :intro %>
  </p>
  <p>
    <%= f.label :creator %><br>
    <%= f.text_field :creator%>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>